<ParameterList name="1D fractional Laplacian">
  <Parameter name="read binary" type="bool" value="false"/>
  <Parameter name="map" type="string" value="1d-mm/rowmap.mtx"/>
  <Parameter name="coefficient map" type="string" value="1d-mm/coeffmap.mtx"/>
  <Parameter name="near colmap" type="string" value="1d-mm/Anear_colmap.mtx"/>
  <Parameter name="ghosted coefficient map" type="string" value="1d-mm/ghosted_coeffmap.mtx"/>
  <Parameter name="aux colmap" type="string" value="1d-mm/aux_colmap.mtx"/>

  <Parameter name="near field matrix" type="string" value="1d-mm/Anear.mtx"/>

  <Parameter name="basis expansion coefficient matrix" type="string" value="1d-mm/basismatrix.mtx"/>
  <Parameter name="far field interaction matrix" type="string" value="1d-mm/kernelApproximations.mtx"/>

  <ParameterList name="shift coefficient matrices">
    <Parameter name="0" type="string" value="1d-mm/transfer0.mtx"/>
    <Parameter name="1" type="string" value="1d-mm/transfer1.mtx"/>
    <Parameter name="2" type="string" value="1d-mm/transfer2.mtx"/>
    <Parameter name="3" type="string" value="1d-mm/transfer3.mtx"/>
    <Parameter name="4" type="string" value="1d-mm/transfer4.mtx"/>
  </ParameterList>

  <Parameter name="right-hand side" type="string" value="1d-mm/RHS.mtx"/>
  <Parameter name="exact solution" type="string" value="1d-mm/X_ex.mtx"/>

  <Parameter name="auxiliary operator" type="string" value="1d-mm/aux.mtx"/>
  <Parameter name="coordinates" type="string" value="1d-mm/coords.mtx"/>
</ParameterList>
