<ParameterList name="1D fractional Laplacian">
  <Parameter name="read binary" type="bool" value="true"/>
  <Parameter name="read local" type="bool" value="false"/>
  <Parameter name="map" type="string" value="1d-binary-global/rowmap.mtx"/>
  <Parameter name="coefficient map" type="string" value="1d-binary-global/coeffmap.mtx"/>
  <Parameter name="near colmap" type="string" value="1d-binary-global/Anear_colmap.mtx"/>
  <Parameter name="ghosted coefficient map" type="string" value="1d-binary-global/ghosted_coeffmap.mtx"/>
  <Parameter name="aux colmap" type="string" value="1d-binary-global/aux_colmap.mtx"/>

  <Parameter name="near field matrix" type="string" value="1d-binary-global/Anear.mtx"/>

  <Parameter name="basis expansion coefficient matrix" type="string" value="1d-binary-global/basismatrix.mtx"/>
  <Parameter name="far field interaction matrix" type="string" value="1d-binary-global/kernelApproximations.mtx"/>

  <ParameterList name="shift coefficient matrices">
    <Parameter name="0" type="string" value="1d-binary-global/transfer0.mtx"/>
    <Parameter name="1" type="string" value="1d-binary-global/transfer1.mtx"/>
    <Parameter name="2" type="string" value="1d-binary-global/transfer2.mtx"/>
    <Parameter name="3" type="string" value="1d-binary-global/transfer3.mtx"/>
    <Parameter name="4" type="string" value="1d-binary-global/transfer4.mtx"/>
  </ParameterList>

  <Parameter name="right-hand side" type="string" value="1d-binary-global/RHS.mtx"/>
  <Parameter name="exact solution" type="string" value="1d-binary-global/X_ex.mtx"/>

  <Parameter name="auxiliary operator" type="string" value="1d-binary-global/aux.mtx"/>
  <Parameter name="coordinates" type="string" value="1d-binary-global/coords.mtx"/>
</ParameterList>
