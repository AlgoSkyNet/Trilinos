<ParameterList name="1D fractional Laplacian">
  <Parameter name="read binary" type="bool" value="false"/>
  <Parameter name="read local" type="bool" value="true"/>
  <Parameter name="map" type="string" value="1d-mm-local/rowmap.mtx"/>
  <Parameter name="coefficient map" type="string" value="1d-mm-local/coeffmap.mtx"/>
  <Parameter name="near colmap" type="string" value="1d-mm-local/Anear_colmap.mtx"/>
  <Parameter name="ghosted coefficient map" type="string" value="1d-mm-local/ghosted_coeffmap.mtx"/>
  <Parameter name="aux colmap" type="string" value="1d-mm-local/aux_colmap.mtx"/>

  <Parameter name="near field matrix" type="string" value="1d-mm-local/Anear.mtx"/>

  <Parameter name="basis expansion coefficient matrix" type="string" value="1d-mm-local/basismatrix.mtx"/>
  <Parameter name="far field interaction matrix" type="string" value="1d-mm-local/kernelApproximations.mtx"/>

  <ParameterList name="shift coefficient matrices">
    <Parameter name="0" type="string" value="1d-mm-local/transfer0.mtx"/>
    <Parameter name="1" type="string" value="1d-mm-local/transfer1.mtx"/>
    <Parameter name="2" type="string" value="1d-mm-local/transfer2.mtx"/>
    <Parameter name="3" type="string" value="1d-mm-local/transfer3.mtx"/>
    <Parameter name="4" type="string" value="1d-mm-local/transfer4.mtx"/>
  </ParameterList>

  <Parameter name="right-hand side" type="string" value="1d-mm-local/RHS.mtx"/>
  <Parameter name="exact solution" type="string" value="1d-mm-local/X_ex.mtx"/>

  <Parameter name="auxiliary operator" type="string" value="1d-mm-local/aux.mtx"/>
  <Parameter name="coordinates" type="string" value="1d-mm-local/coords.mtx"/>
</ParameterList>
